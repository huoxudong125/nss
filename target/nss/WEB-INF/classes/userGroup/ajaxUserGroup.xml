<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="om.feinno.nss.framework.controller.action.userGroup" namespace="/group"  extends="json-default">
        <action name="groupDelete"
            class="com.feinno.nss.framework.controller.action.userGroup.UserGroupAction"
			method="groupDelete">
            <result name="success" type="json"></result>
        </action>
         <action name="groupQuit"
            class="com.feinno.nss.framework.controller.action.userGroup.UserGroupAction"
			method="deleteGroupMapping">
            <result name="success" type="json"></result>
        </action>
         <action name="addMember"
            class="com.feinno.nss.framework.controller.action.userGroup.AddMemberAction"
			method="addMember">
            <result name="success" type="json"></result>
        </action>
          <action name="updateBulletin"
            class="com.feinno.nss.framework.controller.action.userGroup.AddMemberAction"
			method="updateBulletin">
            <result name="success" type="json"></result>
        </action>
        
          <action name="updateIsVerify"
            class="com.feinno.nss.framework.controller.action.userGroup.AddMemberAction"
			method="updateIsVerify">
            <result name="success" type="json"></result>
        </action>
    </package>   
</struts>
